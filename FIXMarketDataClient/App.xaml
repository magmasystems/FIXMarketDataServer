<Application x:Class="FIXMarketDataClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
>
    <!-- With Prism, the MainConsoleWindow is created by the Bootstrapper -->
    <!--    StartupUri="MainConsoleWindow.xaml"> -->

    <Application.Resources>
        <LinearGradientBrush x:Key="MagmaWindowDarkBrush">
            <GradientStop Color="Black" Offset="0"/>
            <GradientStop Color="DarkGray" Offset="1"/>
            <GradientStop Color="Gray" Offset="2"/>
            <GradientStop Color="LightGray" Offset="3"/>
            <GradientStop Color="White" Offset="4"/>
        </LinearGradientBrush>
        <SolidColorBrush x:Key="MagmaSolidBlackWindowBrush" Color="Black" />
    </Application.Resources>
</Application>
