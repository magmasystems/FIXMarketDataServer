<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Collections="clr-namespace:System.Collections;assembly=mscorlib"
    xmlns:FIXMarketDataServer="clr-namespace:FIXMarketDataServer;assembly=MagmaTrader.Data">

    <!-- Value converter from a price change to a color -->
    <!-- Define a list of quotes. We can use this in a grid's binding list -->
    <Collections:ArrayList x:Key="InitialQuoteList">
        <FIXMarketDataServer:Quote Symbol="T"    Bid="34.56" Ask="34.57" />
        <FIXMarketDataServer:Quote Symbol="UTX"  Bid="67.09" Ask="67.10" />
        <FIXMarketDataServer:Quote Symbol="YHOO" Bid="15.00" Ask="15.02" />
    </Collections:ArrayList>

    <LinearGradientBrush x:Key="MagmaWindowDarkBrush">
        <GradientStop Color="Black" Offset="0"/>
        <GradientStop Color="DarkGray" Offset="1"/>
        <GradientStop Color="Gray" Offset="2"/>
        <GradientStop Color="LightGray" Offset="3"/>
        <GradientStop Color="White" Offset="4"/>
    </LinearGradientBrush>

</ResourceDictionary>